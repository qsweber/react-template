{
    "env": {
        "browser": true,
        "es2021": true
    },
    "root": true,
    "extends": [
        "eslint:recommended",
        "react-app"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react",
        "prettier"
    ],
    "rules": {
        "prettier/prettier": "error"
    }
}
